extend includes/page

block link
  -var selected = 'Contacts';

block content
  .atk-wrapper.atk-section
    .atk-cells.atk-push
      .atk-cell.atk-expand
        a.atk-button.atk-swatch-mint(href='#') Add Contact
      .atk-cell.atk-align-right.atk-text-nowrap
        .atk-cells.atk-cells-gutter
          .atk-cell
            span.atk-buttonset
              a.atk-button.atk-button-active.tooltip(href='javascript:void(0)', title="Detailed View") #[span.icon-list-bullet]
              a.atk-button.tooltip(href='javascript:void(0)', title="Compact View") #[span.icon-list]
          .atk-cell
            select(style="width:140px")
              option All (120)
              option Customers (80)
              option Contacts (40)
          .atk-cell
            .atk-input-combo.atk-cells
              .atk-cell
                input(type="text" placeholder="Search Contacts..." style="width:200px")
              .atk-cell
                button.atk-button #[span.icon-search]
