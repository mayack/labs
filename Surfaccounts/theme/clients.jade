            span.atk-buttonset
              a.atk-button.atk-button-active.tooltip(href='javascript:void(0)', title="Detailed View") #[span.icon-list-bullet]
              a.atk-button.tooltip(href='javascript:void(0)', title="Compact View") #[span.icon-list]
          .atk-cell
            select(style="width:140px")
              option All (120)
              option Customers (80)
              option Contacts (40)
          .atk-cell
            .atk-input-combo.atk-cells
