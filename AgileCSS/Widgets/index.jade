    meta(name="viewport", content="width=device-width, initial-scale=1")
    title Agile CSS
    link(rel='stylesheet', href='css/widgets.css')
  body
    .atk-layout
      .atk-layoutRow.atk-layoutExpand
        .atk-layout.atk-layoutCell
          main.atk-layoutColumn.atk-layoutExpand.atk-align-middle
            header#toolbar.atk-padding--xs.atk-text-caps-semibold-baseline--single.atk-comment
              a.atk-cells-spacing--xs.atk-move-left(href="javascript:void(0)")
                span.atk-cell
                  span.icon-angle-left.atk-text-size--l
                span.atk-cell
                  span.atk-text-size--xs Widget Gallery
              a#inspector-trigger.atk-cells-spacing--xs.atk-move-right(href="javascript:void(0)")
                span.atk-cell
                  span.atk-text-size--xs Show Code
                span.atk-cell
                  span.icon-code.atk-text-size--l
            iframe.atk-text-font--clear(src="../Social Feed/index.html" width="100%" height="100%" frameborder="0")
          aside#inspector.atk-layoutColumn.atk-swatch-ink
            #inspector-inner
              header.atk-text-caps-size--xs-semibold.atk-border-bottom-disabled.atk-align-center.atk-padding--xs
                a#inspector-hide.icon-cancel(href="javascript:void(0)")
                | HTML Code
              pre
                code.html
                  include includes/snippet.jade
